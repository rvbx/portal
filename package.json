{
  "name": "portal-rvbx",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run install:design-styles",
    "start": "ng serve",
    "build": "ng build",
    "build-lib:styles": "node projects/rvbx-design-styles/copy-styles.js && cd dist/rvbx-design-styles",
    "build-lib:system": "ng build --project=rvbx-design-system && cd dist/rvbx-design-system",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:design-styles": "npm install --save projects/rvbx-design-styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@rvbx/design-styles": "file:projects/rvbx-design-styles",
    "@rvbx/design-system": "file:projects/rvbx-design-system",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copy": "^0.3.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
